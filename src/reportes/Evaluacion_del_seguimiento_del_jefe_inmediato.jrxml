	<subDataset name="obstaculo_ds_1" uuid="6615673c-3b8f-45dc-828b-62af6a20431e">
		<parameter name="idparticipante_obstaculo1" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
case COALESCE((select obstaculo
from obstaculo_aplicacion INNER JOIN obstaculoaplicacion_evaluacionjefe
on obstaculo_aplicacion.idobstaculo_aplicacion=obstaculoaplicacion_evaluacionjefe.idobstaculo_aplicacion
INNER JOIN evaluacion_jefe on obstaculoaplicacion_evaluacionjefe.idevaluacion_jefe=evaluacion_jefe.idevaluacion_jefe INNER JOIN participante on evaluacion_jefe.idevaluacion_jefe=participante.idevaluacion_jefe
WHERE participante.idparticipante=$P{idparticipante_obstaculo1} and obstaculo_aplicacion.idobstaculo_aplicacion=1),'')
when 'FALTA DE EQUIPO Y/O MATERIAL' then
'X' else ' '
end as o_P,
case COALESCE((select obstaculo
from obstaculo_aplicacion INNER JOIN obstaculoaplicacion_evaluacionjefe
on obstaculo_aplicacion.idobstaculo_aplicacion=obstaculoaplicacion_evaluacionjefe.idobstaculo_aplicacion
INNER JOIN evaluacion_jefe on obstaculoaplicacion_evaluacionjefe.idevaluacion_jefe=evaluacion_jefe.idevaluacion_jefe INNER JOIN participante on evaluacion_jefe.idevaluacion_jefe=participante.idevaluacion_jefe
WHERE participante.idparticipante=$P{idparticipante_obstaculo1} and obstaculo_aplicacion.idobstaculo_aplicacion=2),'')
when 'FALTA DE APOYO EN EL ÁREA DE TRABAJO' then
'X' else ' '
end as o_P2,
case COALESCE ((select count(evaluacion_jefe.otro_obstaculo)
FROM evaluacion_jefe INNER JOIN participante
on evaluacion_jefe.idevaluacion_jefe=participante.idevaluacion_jefe
where participante.idparticipante=$P{idparticipante_obstaculo1}))
when '1' then
'X' else ''
end as obstaculo_1
from participante where idparticipante=$P{idparticipante_obstaculo1}]]>
		</queryString>
		<field name="o_p" class="java.lang.String"/>
		<field name="o_p2" class="java.lang.String"/>
		<field name="obstaculo_1" class="java.lang.String"/>
	</subDataset>
